{
  "server": "hub-cloud.browserstack.com",
  "user": "BROWSERSTACK_USERNAME",
  "key": "BROWSERSTACK_ACCESS_KEY",

  "capabilities": {
    "project": "SE Demo",
    "build": "TestNG Samples",
    "browserstack.debug": true
  },

  "environments": {
    "chrome": {
      "os": "Windows",
      "os_version": "10",
      "browser": "Chrome",
      "browser_version": "78.0",
      "resolution": "1024x768",
      "name": "parallel run - Chrome"
    },
    "firefox": {
      "os": "Windows",
      "os_version": "10",
      "browser": "Firefox",
      "browser_version": "70.0",
      "resolution": "1024x768",
      "name": "parallel run - Firefox"
    },
    "android": {
      "device": "Samsung Galaxy Note 10 Plus",
      "realMobile": "true",
      "os_version": "9.0",
      "name": "parallel run - Android"
    },
    "ie": {
      "os": "Windows",
      "os_version": "7",
      "browser": "IE",
      "browser_version": "11.0",
      "resolution": "1024x768",
      "name": "parallel run - Internet Explorer"
    }
  }
}
